<ul class="crumbs">
	<li><a href="/">Home</a></li>
	<li>Users</li>
</ul>
<article>
	<header>
		<h1>Users</h1>
	</header>
	<table>
		<thead>
			<tr>
				<th>Name</th>
				<th>Email</th>
			</tr>
		</thead>
		<tbody>
			{{#users}}<tr class="link" onclick="moostaka.navigate('/users/{{id}}');">
				<td>{{name}}</td><td>{{email}}</td>
			</tr>{{/users}}
			{{^users}}<tr><td colspan="2">No users found.</td></tr>{{/users}}
		</tbody>
	</table>
</article>